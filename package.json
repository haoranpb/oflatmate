{
  "name": "flatmate",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "functions"
  ],
  "scripts": {
    "ci": "yarn install --frozen-lockfile --ignore-engines",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "emulate": "firebase emulators:start --import=./tmp-data --export-on-exit",
    "deploy:func": "firebase deploy --only functions",
    "env": "yarn env:unset && yarn env:set",
    "env:set": "firebase functions:config:set env=\"$(cat functions/env.json)\"",
    "env:unset": "firebase functions:config:unset env",
    "env:get": "firebase functions:config:get > functions/env.json"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9.8.6",
    "core-js": "^3.6.5",
    "dompurify": "^2.2.6",
    "firebase": "^8.2.1",
    "firebaseui": "^4.7.3",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "validator": "^13.5.2",
    "vue": "^3.0.4",
    "vue-router": "^4.0.2",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-plugin-router": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.4.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
